LDlinkR::SNPclip

# Funtion call, good and bad examples
myfile <- "/Volumes/ifs/DCEG/Branches/LTG/Chanock/Tim/LDlinkR/SNPclipR/data_saved/text2.txt"

snpclip(c("rs3", "rs4", "rs148890987"), "YRI", "0.1", "0.01", "28da99809470", myfile)         # good, w/ save file option
snpclip(c("rs3", "rs4", "rs148890987"), "YRI", "0.1", "0.01", "28da99809470")                 # good
snpclip(c("chr13:32446842", "Rs4", "rs148890987"), "YRI", "0.1", "0.01", "28da99809470")      # good, one SNP w/ genomic coordinate
snpclip(c("ch13:32446842", "Rs4", "rs148890987"), "YRI", "0.1", "0.01", "28da99809470")       # good, one SNP w/ genomic coordinate
snpclip(c("chr13:32446842", "Rs4", "rs148890987"), "YRI", token = "28da99809470")             # good, one SNP w/ genomic coordinate, no thresholds
snpclip(c("rs3", "rs4", "rs148890987"), "YRI", 0.1, "0.01", "28da99809470")                   # bad, r2_theshold is numeric
snpclip(c("rs3", "rs4", "rs148890987"), "YRI", "0.1", 0.01, "28da99809470")                   # good, maf_theshold is numeric

