LDlinkR::LDproxy
# Funtion call, good and bad examples
# token="28da99809470"
myfile <- "/Volumes/ifs/DCEG/Branches/LTG/Chanock/Tim/LDlinkR/LDproxyR/data_saved/capture_text1.txt"

df_proxy <- LDproxy("rs456", "YRI", "r2", "28da99809470", myfile)           # good, save file option
df_proxy <- LDproxy("rs456", "YRI", "r2", "28da99809470", 2345.txt)         # bad, save file option
df_proxy <- LDproxy("rs456", "YRI", "r2", "28da99809470", "capture3.txt")   # good, save file

df_proxy <- LDproxy("rs456", "YRI", "r2", "28da99809470")             # good
df_proxy <- LDproxy("rr456", "YRI", "r2", "28da99809470")             # bad query SNP format
df_proxy <- LDproxy("rs1", "YRI", "r2", "28da99809470")               # invalid SNP
df_proxy <- LDproxy("chr7:24966446", "YRI", token = "28da99809470")   # good, uses chromosome coordinate
df_proxy <- LDproxy("chr7:24966446", "YRI", token = "faketoken")      # fake token
df_proxy <- LDproxy("ch7:24966446", "YRI", token = "28da99809470")    # bad query SNP format
df_proxy <- LDproxy("chr7:24966446", "YRI")                           # missing token
df_proxy <- LDproxy("rs345", "YRI", "r2", "28da99809470")             # good
df_proxy <- LDproxy("rs7837688", "MXL", "d", "28da99809470")          # good, using 'MXL' and 'd'
df_proxy <- LDproxy("rs456", "YRI", "r2")                             # missing token
df_proxy <- LDproxy("rs456", c("YRI", "MXL"), "r2", "28da99809470")   # good, using multiple populations
df_proxy <- LDproxy("rs456", c("YRI", "TIM"), "r2", "28da99809470")   # bad population in multiple populations
df_proxy <- LDproxy("Chr7:24966446", "YRI", token = "28da99809470")   # good, uses chromosome coordinate, with upper case
df_proxy <- LDproxy("chr7;24966446", "YRI", token = "28da99809470")   # bad, uses chromosome coordinate with semi-colon
df_proxy <- LDproxy("456rs", "YRI", "r2", "28da99809470")             # bad query SNP format
df_proxy <- LDproxy("rs75837874", "EUR", "r2", "28da99809470")        # NOT BIALLELIC in EUR population, error message


# Batch call
snps.to.upload <- c("rs123", "rs456", "rs7837688")
  for(i in 1:length(snps.to.upload)) {
    myfile <- paste(snps.to.upload[i], ".txt", sep="")
    df_proxy <- LDproxy(snp=snps.to.upload[i], "YRI", "r2", "28da99809470", myfile)
    # write.table(df_proxy, file = file, quote = F, row.names = F, sep = "\t")
  }


