LDlinkR::LDpair
# Funtion call, good and bad examples
# token="28da99809470"
myfile <- "/Volumes/ifs/DCEG/Branches/LTG/Chanock/Tim/LDlinkR/LDpairR/data_saved/capture_text12.txt"
mytable <- "/Volumes/ifs/DCEG/Branches/LTG/Chanock/Tim/LDlinkR/LDpairR/data_saved/table12.txt"

LDpair("rs3", "rs4", "YRI", "28da99809470", "text", file = myfile)                          # good, no warning, w/ text and save file options
LDpair("rs3", "rs4", "YRI", "28da99809470", "table", file = mytable)                        # good, no warning, w/ table and save file options
LDpair("rs3", "rs456", "YRI", "28da99809470", "text", file = myfile)                        # good, w/ warning, w/ text and save file options
LDpair("rs3", "rs456", "YRI", "28da99809470", "table", file = mytable)                      # good, w/ warning, w/ table and save file options

LDpair("rs3", "rs4", "YRI", "28da99809470", "txt", file = mytable)                          # bad, invalid input for "output" option
LDpair("rs3", "rs4", "YRI", "28da99809470", "text", file = TRUE)                            # bad, invalid input for "file" option
LDpair("rs3", "rs4", "YRI", "28da99809470", "text", file = 1)                               # bad, invalid input for "file" option

LDpair("rs3", "rs456", "YRI", "28da99809470", "text")                                       # warning, variants on different chr's
LDpair("rs3", "rs456", "YRI", "28da99809470", "table")                                      # warning, variants on different chr's
LDpair("rs3", "rs4", "YRI", "28da99809470", "text")                                         # good, no warning, w/ text option
LDpair("rs3", "rs4", "YRI", "28da99809470", "table")                                        # good, no warning, w/ text option

LDpair(var1 = "rs3", var2 = "rs4", pop = "YRI", token = "28da99809470")                     # good
LDpair(var1 = "rs3", var2 = "rr4", pop = "YRI", token = "28da99809470")                     # bad, invalid query variant 2
LDpair(var1 = "r3", var2 = "rs4", pop = "YRI", token = "28da99809470")                      # bad, invalid query variant 1
LDpair("rs3", "rs456", "YRI", "28da99809470")                                               # warning, variants on different chr's
LDpair("rs456", "YRI", "28da99809470")                                                      # bad, only 1 variant
LDpair("chr13:32446842", "rs4", "YRI", "28da99809470")                                      # good, chr coordinate for variant 1
LDpair("chr13:32446842", "chr13:32446842", "YRI", "28da99809470")                           # good, chr coordinate for both variants
LDpair("ch13:32446842", "chr13:32447222", "YRI", "28da99809470")                            # bad, invalid format for variant 1
LDpair("chr1332446842", "chr13:32447222", "YRI", "28da99809470")                            # bad, invalid format for variant 1
LDpair("chr13:32446842", "13:32447222", "YRI", "28da99809470")                              # bad, invalid format for variant 2
LDpair("rs3", "rs4", c("YRI", "LWK", "CEU"), "28da99809470")                                # good, w/ multiple populations
LDpair("rs0", "rs4", c("YRI", "LWK", "CEU"), "28da99809470")                                # bad, rsID is not in dbSNP
LDpair(var1 = c("rs3", "rs456"), var2 = "rs4", pop = "YRI", token = "28da99809470")         # bad, multiple variants for variant 1
LDpair(var1 = "rs3", var2 = "Rs4", pop = "YRI", token = "28da99809470")                     # good, upper case in rsID
LDpair("Chr13:32446842", "rs4", "YRI", "28da99809470")                                      # good, upper case in chr coordinate


##########################################################################################

LDlinkR::LDpop
# Funtion call, good and bad examples
# token="28da99809470"
myfile <- "/Volumes/ifs/DCEG/Branches/LTG/Chanock/Tim/LDlinkR/SNPchipR/data_saved/text1.txt"

LDpop(var1 = "rs3", var2 = "rs4", pop = "YRI", r2d = "r2", token = "28da99809470", myfile)                   # good, with save file option
LDpop(var1 = "rs3", var2 = "rs4", pop = "YRI", r2d = "r2", token = "28da99809470", "text6.txt")              # good, with save file option

LDpop(var1 = "rs3", var2 = "rs4", pop = "YRI", r2d = "r2", token = "28da99809470")                           # good
LDpop(var1 = "Rs3", var2 = "rs4", pop = "YRI", r2d = "r2", token = "28da99809470")                           # good, upper case in var1 rsID
LDpop(var1 = "rs3", var2 = "Rs4", pop = "YRI", r2d = "r2", token = "28da99809470")                           # good, upper case in var2 rsID
LDpop(var1 = "s3", var2 = "Rs4", pop = "YRI", r2d = "r2", token = "28da99809470")                            # bad, invalid var2 rsID
LDpop(var1 = "rs3", var2 = "R4", pop = "YRI", r2d = "r2", token = "28da99809470")                            # bad, invalid var2 rsID
LDpop(var1 = "chr13:32446842", var2 = "chr13:32446842", pop = "YRI", r2d = "r2", token = "28da99809470")     # good, using chr coordinates
LDpop(var1 = "rs3", var2 = "chr13:32446842", pop = "YRI", r2d = "r2", token = "28da99809470")                # good, using chr coordinates
LDpop(var1 = "Chr13:32446842", var2 = "chr13:32446842", pop = "YRI", r2d = "r2", token = "28da99809470")     # good, upper case var1 chr coordinate
LDpop(var1 = "Chr13:32446842", var2 = "CHR13:32446842", pop = "YRI", r2d = "r2", token = "28da99809470")     # good, upper case for var1 & var2
LDpop(var1 = "ch13:32446842", var2 = "chr13:32446842", pop = "YRI", r2d = "r2", token = "28da99809470")      # bad, invalid var1 chr coordinate
LDpop(var1 = "chr13:32446842", var2 = "cr13:32446842", pop = "YRI", r2d = "r2", token = "28da99809470")      # bad, invalid var2 chr coordinate
LDpop(var1 = "rs0", var2 = "rs4", pop = "YRI", r2d = "r2", token = "28da99809470")                           # good, error for var1 is not in dbSNP 151
LDpop(var1 = "rs3", var2 = "rs456", pop = "YRI", r2d = "r2", token = "28da99809470")                         # good, 
pop_out <- LDpop(var1 = "rs3", var2 = "chr13:32446842", pop = "YRI", r2d = "r2", token = "28da99809470")     # good, using chr coordinates
LDpop(var1 = "rs72630805", var2 = "rs189192078", pop = "YRI", r2d = "r2", token = "28da99809470")            # bad, rsID on chrX


