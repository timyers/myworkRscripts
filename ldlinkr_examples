LDlinkR::SNPchip
# Funtion call, good and bad examples
myfile <- "/Volumes/ifs/DCEG/Branches/LTG/Chanock/Tim/LDlinkR/SNPchipR/data_saved/table5.txt"

snpchip(c("rs3", "rs4", "rs148890987"), "ALL", "28da99809470", myfile)                       # good, w/ save file option

snpchip(c("rs3", "rs4", "rs148890987"), "ALL", "28da99809470")                               # good
snpchip("rs3", "ALL", "28da99809470")                                                        # good, only one SNP provided
snpchip(c("rs3", "rs4", "rs148890987"), c("A_CHB2", "A_SNP5.0"), "28da99809470")             # good, multiple multiple chip codes
snpchip(c("rs3", "rs4", "rs148890987"), c("YRI", "CEU"), "faketoken")                        # bad, fake token with multiple populations
snpchip(c("rs3", "rs4", "rs148890987"), "ALL", "faketoken")                                  # bad, fake token with one population
snpchip(c("rs3", "rs4", "rs148890987"), "ALL")                                               # bad, fake token with one population
snpchip(c("r3", "rs4", "rs148890987"), "ALL", "28da99809470")                                # bad query SNP format
snpchip(c("rs0", "rs4", "rs148890987"), "XYZ", "28da99809470")                               # bad chip code  
snpchip(c("rs0", "rs4"), "ALL", "28da99809470")                                              # bad with good response, used one valid & invalid SNP  
snpchip(c("rs3", "rs4"), "A_SNP6.0", "28da99809470")                                         # good, with two valid variants
snpchip(c("chr13:32446842", "Rs4", "rs148890987"), "ALL", "28da99809470")                    # good, one SNP w/ genomic coordinate
snpchip(c("chr13:32446842", "chr13:32447222", "rs148890987"), "ALL", "28da99809470")         # good, two SNPs w/ genomic coordinate
snpchip(c("ch13:32446842", "chr13:32447222", "rs148890987"), "ALL", "28da99809470")          # bad, first SNP w/ invalid genomic coordinate
snpchip(c("chr13:32446842", "cr13:3244722", "rs148890987"), "ALL", "28da99809470")           # bad, 2nd SNP w/ invalid genomic coordinate
snpchip(c("rs3", "rs4", "rs148890987"), "ALL_Illumina", "28da99809470")                      # good, w/ ALL_Illumina, no arrays found
snpchip(c("rs3", "rs4", "rs148890987"), "ALL_Illumina", "28da99809470", myfile)              # good, w/ ALL_Illumina, no arrays found
snpchip(c("rs3", "rs4", "rs148890987"), "ALL_Affy", "28da99809470")                          # good, w/ ALL_Affy
snp_table <- snpchip(c("rs3", "rs4", "rs355"), "ALL", "28da99809470")                        # good
snpchip(c("rs4", "rs355"), "ALL", "28da99809470")                                            # good
snpchip(c("rs3", "rs4", "rs355"), "ALL", "28da99809470")                                     # good
snpchip(c("rs3", "rs4", "rs355", "rs2314396"), "ALL", "28da99809470")                        # good
snpchip(c("rs3", "rs4", "rs355", "rs148890987", "rs2314396"), "ALL", "28da99809470")         # good


